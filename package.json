    "isbot": "^5.1.31",
    "react": "^19.1.1",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^19.1.1",
    "react-router": "^7.9.2",
    "scule": "^1.3.0",
    "styled-components": "^6.1.19",
    "valtio": "^2.2.0"
  },
  "devDependencies": {
    "@react-router/dev": "^7.9.2",
    "@tailwindcss/vite": "^4.1.13",
